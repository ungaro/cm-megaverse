{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "dist",
    "strict": true,
    "target": "ES2020",                               
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
  },
  "include": ["src/**/*.ts", "tests/**/*.ts"]
}


